class Dorama:
    def __init__(self, name: str, comments: str):
        self.name = name
        self.comments = comments

    def __str__(self):
        return f"{self.name} - {self.comments}"


class Anime:
    def __init__(self, name: str, comments: str):
        self.name = name
        self.comments = comments

    def __str__(self):
        return f"{self.name}- {self.comments}"
